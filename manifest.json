{
  "manifest_version": 2,
  "name": "The not evil youtube",
  "version": "2.0.1",
  "description": "Removes unwanted videos",
  "icons": {
    "48": "yt_icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "yt_icon.png",
    "default_title": "DBE",
    "default_popup": "index.html"
  }
}

